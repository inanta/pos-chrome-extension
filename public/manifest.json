{
  "name": "OOC PlatformOS Chrome Extension",
  "description": "PlatformOS Chrome Extension",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting", "tabs"],
  "action": {
    "default_icon": {
      "16": "/img/ooc16.png",
      "32": "/img/ooc32.png",
      "48": "/img/ooc48.png",
      "128": "/img/ooc128.png"
    }
  },
  "icons": {
    "16": "/img/ooc16.png",
    "32": "/img/ooc32.png",
    "48": "/img/ooc48.png",
    "128": "/img/ooc128.png"
  }
}
